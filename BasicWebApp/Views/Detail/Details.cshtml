@model BasicWebApp.Services.MetMumbaiDbContext
<html>
    <head>
        <title>BasicWebApp</title>
    </head>
    <body>
        <center>

        <h1>Students In This Course </h1>
        <table border="2">
            <tr>
                <th>Course Id</th>
                <th>Student Id</th>
                <th>Student Name</th>
                <th>Student Age</th>
                <th>Grade</th>
            </tr>
            @foreach (var entry in ViewBag.model)
            {
                <tr>
                    <td>@entry.CourseId</td>
                    <td>@entry.Id</td>
                    <td>@entry.StudentName</td>
                    <td>@entry.Age</td>
                    <td>@entry.Grade</td>
                </tr>
            }
        </table>
        </center>
    </body>
</html>